{
  "name": "@okgrow/graphql-scalars",
  "version": "0.4.5",
  "description": "A collection of scalar types not included in base GraphQL.",
  "repository": {
    "type": "git",
    "url": "https://github.com/okgrow/graphql-scalars.git"
  },
  "typings": "graphql-scalars.d.ts",
  "license": "MIT",
  "jest": {
    "roots": [
      "src"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 80
  },
  "scripts": {
    "commit": "commit",
    "prepublishOnly": "npm run build",
    "build": "babel src --ignore '**/__tests__' --out-dir ./dist",
    "lint": "eslint ./src",
    "lintfix": "eslint ./src --fix",
    "test-only": "jest",
    "test": "npm run lint && jest",
    "test-watch": "npm run test-only -- --watch --watchAll",
    "start": "babel src -d dist --watch"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/preset-env": "^7.3.1",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@commitlint/prompt-cli": "8.3.5",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.12.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-react": "^7.12.4",
    "graphql": "^14.1.1",
    "husky": "4.2.3",
    "jest": "^24.0.0"
  },
  "peerDependencies": {
    "graphql": ">=0.13.1"
  }
}
